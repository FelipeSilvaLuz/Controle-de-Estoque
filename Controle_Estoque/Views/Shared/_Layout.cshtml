@{
    bool verLayout = ViewBag.verBotoes ?? false;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Controle_Estoque</title>

    <link href="~/css/bootstrap.min.css" rel="stylesheet" />

    <script src="~/js/jquery-1.12.2.min.js"></script>
    <script src="~/js/jquery-ui.js"></script>
    <script src="~/js/jquery.mask.js"></script>
    <script src="~/js/bootstrap.min.js"></script>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>


</head>
<body>
    <partial name="_CookieConsentPartial" />

    <div class="container body-content col-md-12">
        @if (verLayout)
        {
            <div class="col-md-12 painel_central_superior">
                <div class="titulo_layout col-md-6"> Sistema de Controle de Estoque </div>

                <div class="glyphicon glyphicon-cog" style="float:right;color:black; margin-top:12px;"></div>
                <a href="#" class="" style="float:right; margin-right:20px; margin-top:10px;">LogOut</a>
            </div>

            <div class="col-md-12" id="tabsTop">
                <div class="board">
                    <div class="board-inner">
                        <div class="painel_lateral">
                            <ul class="nav nav-tabs">
                                <li class="active">
                                    <a href="#tabsTop-1" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs one">
                                            <em class="glyphicon glyphicon-briefcase"> Produtos </em>
                                        </span>
                                    </a>
                                </li>
                                <div style="margin-top:20px;"></div>
                                <li>
                                    <a href="#tabsTop-2" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs two">
                                            <em class="glyphicon glyphicon-wrench"> Serviços </em>
                                        </span>
                                    </a>
                                </li>
                                <div style="margin-top:20px;"></div>
                                <li>
                                    <a href="#tabsTop-3" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs three">
                                            <em class="glyphicon glyphicon-folder-open"> Estoque </em>
                                        </span>
                                    </a>
                                </li>
                                <div style="margin-top:20px;"></div>
                                <li>
                                    <a href="#tabsTop-4" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs four">
                                            <em class="glyphicon glyphicon-plane"> Logistica </em>
                                        </span>
                                    </a>
                                </li>
                                <div style="margin-top:20px;"></div>
                                <li>
                                    <a href="#tabsTop-5" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs five">
                                            <em class="glyphicon glyphicon-barcode"> Vendas </em>
                                        </span>
                                    </a>
                                </li>
                                <div style="margin-top:20px;"></div>
                                <li>
                                    <a href="#tabsTop-6" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs six">
                                            <em class="glyphicon glyphicon-book"> Relatorios </em>
                                        </span>
                                    </a>
                                </li>
                                <div style="margin-top:20px;"></div>
                                <li>
                                    <a href="#tabsTop-7" data-toggle="tab" class="btn btn-dark">
                                        <span class="round-tabs six">
                                            <em class="glyphicon glyphicon-flag"> Estatisticas </em>
                                        </span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="tab-content">
                            <div class="tab-pane active" id="tabsTop-1">
                                @Html.Partial("Produtos")
                            </div>

                            <div class="tab-pane" id="tabsTop-2">
                                @Html.Partial("Servicos")
                            </div>

                            <div class="tab-pane" id="tabsTop-3">
                                @Html.Partial("Estoque")
                            </div>

                            <div class="tab-pane" id="tabsTop-4">
                                @Html.Partial("Logistica")
                            </div>

                            <div class="tab-pane" id="tabsTop-5">
                                @Html.Partial("Vendas")
                            </div>

                            <div class="tab-pane" id="tabsTop-6">
                                @Html.Partial("Relatorios")
                            </div>

                            <div class="tab-pane" id="tabsTop-7">
                                @Html.Partial("Estatisticas")
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        }

        @RenderBody()
    </div>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-3.3.1.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
        </script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-aJ21OjlMXNL5UyIl/XNwTMqvzeRMZH2w8c5cRVpzpU8Y5bApTppSuUkhZXN0VxHd">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>